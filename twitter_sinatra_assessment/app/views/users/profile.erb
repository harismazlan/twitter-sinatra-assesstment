<h2>User Profile</h2>

<h3>User Information</h3>


 <p><%=  "User ID: #{@user.id}" %></p>
 <p><%=  "E-mail: #{@user.email}" %></p>
 <p><%=  "User since: #{@user.created_at}" %></p>
 <a href="<%=@user.id%>/twits">View all twits by this user</a>

<% if logged_in?%>
	<form method="post" action="/sessions/delete">
	    <input type=submit value="Log Me Out">
	</form>
<% end%>
